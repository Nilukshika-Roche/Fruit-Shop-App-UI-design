    <ImageView
        android:id="@+id/imageView44"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/homee"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/recyclerView14"
        app:layout_constraintHorizontal_bias="0.068"
        app:layout_constraintStart_toStartOf="@+id/recyclerView14"
        app:layout_constraintTop_toTopOf="@+id/recyclerView14"
        app:layout_constraintVertical_bias="0.157" />

    <ImageView
        android:id="@+id/imageView43"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:background="@drawable/book"
        app:layout_constraintBottom_toBottomOf="@+id/recyclerView14"
        app:layout_constraintEnd_toEndOf="@+id/recyclerView14"
        app:layout_constraintHorizontal_bias="0.65"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/recyclerView14"
        app:layout_constraintVertical_bias="0.604" />

    <ImageView
        android:id="@+id/imageView45"
        android:layout_width="27dp"
        android:layout_height="25dp"
        android:layout_marginTop="28dp"
        android:background="@drawable/cart"
        app:layout_constraintEnd_toEndOf="@+id/recyclerView14"
        app:layout_constraintHorizontal_bias="0.939"
        app:layout_constraintStart_toStartOf="@+id/recyclerView14"
        app:layout_constraintTop_toTopOf="@+id/recyclerView14" />

</androidx.constraintlayout.widget.ConstraintLayout>