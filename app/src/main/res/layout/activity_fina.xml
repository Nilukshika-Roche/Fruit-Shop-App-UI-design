<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fina">

    <Button
        android:id="@+id/button3"
        android:layout_width="196dp"
        android:layout_height="48dp"
        android:backgroundTint="@color/yellow"
        android:text="Track order"
        app:layout_constraintBottom_toBottomOf="@+id/recyclerView26"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.809" />

    <Button
        android:id="@+id/button4"
        android:layout_width="196dp"
        android:layout_height="48dp"
        android:backgroundTint="@color/yellow"
        android:text="Continue Shopping"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.878" />

    <TextView
        android:id="@+id/textView42"
        android:layout_width="288dp"
        android:layout_height="51dp"
        android:text="Order Confirmation"
        android:textColor="@color/yellow"
        android:textSize="28sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/recyclerView26"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.626"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.079" />

    <TextView
        android:id="@+id/textView43"
        android:layout_width="358dp"
        android:layout_height="58dp"
        android:text="Thank you for your order!"
        android:textColor="@color/black"
        android:textSize="28sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.739"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.491" />

    <TextView
        android:id="@+id/textView44"
        android:layout_width="256dp"
        android:layout_height="112dp"
        android:text="Your order has been placed successfully and will reach your place on time"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.451"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.602" />

    <TextView
        android:id="@+id/textView45"
        android:layout_width="272dp"
        android:layout_height="67dp"
        android:text="Enjoy your fresh orgnnic food!"
        android:textAlignment="center"
        android:textColor="@color/gray"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/recyclerView26"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.748" />

    <ImageView
        android:id="@+id/imageView65"
        android:layout_width="228dp"
        android:layout_height="228dp"
        android:background="@drawable/ta"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.233" />


    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerView26"
        android:layout_width="433dp"
        android:layout_height="67dp"
        android:background="@drawable/nav"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <ImageView
        android:id="@+id/imageView66"
        android:layout_width="27dp"
        android:layout_height="28dp"
        android:background="@drawable/proff"
        app:layout_constraintBottom_toBottomOf="@+id/recyclerView26"
        app:layout_constraintEnd_toEndOf="@+id/imageView67"
        app:layout_constraintHorizontal_bias="0.385"
        app:layout_constraintStart_toStartOf="@+id/recyclerView26"
        app:layout_constraintTop_toBottomOf="@+id/button4"
        app:layout_constraintVertical_bias="0.831" />

    <ImageView
        android:id="@+id/imageView69"
        android:layout_width="27dp"
        android:layout_height="28dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/homee"
        app:layout_constraintEnd_toEndOf="@+id/imageView67"
        app:layout_constraintHorizontal_bias="0.101"
        app:layout_constraintStart_toStartOf="@+id/recyclerView26"
        app:layout_constraintTop_toTopOf="@+id/recyclerView26" />

    <ImageView
        android:id="@+id/imageView68"
        android:layout_width="27dp"
        android:layout_height="28dp"
        android:background="@drawable/book"
        app:layout_constraintBottom_toBottomOf="@+id/recyclerView26"
        app:layout_constraintEnd_toEndOf="@+id/imageView67"
        app:layout_constraintHorizontal_bias="0.709"
        app:layout_constraintStart_toStartOf="@+id/recyclerView26"
        app:layout_constraintTop_toBottomOf="@+id/button4"
        app:layout_constraintVertical_bias="0.831" />

    <ImageView
        android:id="@+id/imageView67"
        android:layout_width="27dp"
        android:layout_height="28dp"
        android:background="@drawable/cart"
        app:layout_constraintBottom_toBottomOf="@+id/recyclerView26"
        app:layout_constraintEnd_toEndOf="@+id/recyclerView26"
        app:layout_constraintHorizontal_bias="0.94"
        app:layout_constraintStart_toStartOf="@+id/recyclerView26"
        app:layout_constraintTop_toTopOf="@+id/recyclerView26"
        app:layout_constraintVertical_bias="0.641" />

</androidx.constraintlayout.widget.ConstraintLayout>